{
  "name": "music-box-fun",
  "version": "1.0.0",
  "description": "A web app for making music box songs",
  "author": "Bryan Braun",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bryanbraun/music-box-fun.git"
  },
  "scripts": {
    "test": "cypress run",
    "test-prod": "cypress run --config baseUrl=https://musicboxfun.com",
    "test-visual": "cypress open",
    "build": "node --experimental-modules build-modules.mjs",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "cypress": "^5.2.0",
    "esinstall": "^1.1.7"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "json-url": "2.4.0",
    "tone": "^14.7.58"
  },
  "buildModules": {
    "install": [
      "classnames",
      "tone",
      "json-url"
    ],
    "installOptions": {
      "dest": "public/js/vendor"
    }
  }
}
